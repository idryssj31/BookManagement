{
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "cypress": "^14.3.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "copy:js": "xcopy /E /I /Y build\\static\\js\\* ..\\src\\main\\resources\\static\\js\\",
    "copy:all": "xcopy /E /I /Y build\\* ..\\src\\main\\resources\\static\\",
    "watch:copy": "chokidar \"build/index.html\" \"build/static/**/*\" -c \"xcopy /E /I /Y build\\index.html ..\\src\\main\\resources\\static\\ & xcopy /E /I /Y build\\static\\* ..\\src\\main\\resources\\static\\\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
